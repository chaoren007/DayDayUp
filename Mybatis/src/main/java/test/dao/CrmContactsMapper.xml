<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="test.dao.ICrmContactsDao" >


    <select id="getCrmContactsById" resultMap="CrmContactsMap" parameterType="java.lang.Long" >
        select *
          from t_crm_contacts
         where contacts_id = #{contactsId,jdbcType=BIGINT}
    </select>
    
    <select id="getAll"  resultMap="CrmContactsMap" parameterType="test.bean.CrmContacts">
        select <include refid="Base_Column_List" /> from t_crm_contacts where 1=1
        <include refid="normalWhere" />
    </select>

</mapper>